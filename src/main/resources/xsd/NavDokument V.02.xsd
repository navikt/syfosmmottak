									<xs:documentation>avsenderRef</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Ebms" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ebXMLSamtakleId"/>
