				<xs:extension base="nd:NavInnDokumentType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NavArkivDokument">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="nd:NavArkivDokumentType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>	
	<xs:complexType name="navnType">
		<xs:sequence>
			<xs:element name="fornavn" type="xs:string" minOccurs="0"/>
